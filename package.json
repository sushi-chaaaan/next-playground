{
  "name": "next-playground",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "fix": "yarn run lint:fix && yarn run format",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,md,mdx}'",
    "preinstall": "typesync || :",
    "postinstall": "yarn run sort-package-json",
    "lint": "next lint",
    "lint:fix": "yarn lint --fix",
    "start": "next start",
    "up": "yarn run fix && yarn dev"
  },
  "dependencies": {
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@next/font": "13.1.6",
    "@next/mdx": "^13.1.6",
    "next": "13.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-spotify-embed": "^1.0.4",
    "react-twitter-embed": "^4.0.4",
    "rehype-highlight": "^6.0.0",
    "remark-gfm": "^3.0.1"
  },
  "devDependencies": {
    "@types/eslint": "8.21.1",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/node": "18.14.0",
    "@types/prettier": "^2.7.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "eslint": "8.34.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^2.8.4",
    "sort-package-json": "^2.4.1",
    "typescript": "4.9.5",
    "typesync": "^0.10.0"
  },
  "volta": {
    "node": "18.14.1",
    "yarn": "1.22.19"
  }
}
